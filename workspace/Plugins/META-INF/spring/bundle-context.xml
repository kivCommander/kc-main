<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
                           http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="createFilePlugin" class="cz.zcu.kiv.kc.plugin.create.CreateFilePlugin">
		<property name="eventAdmin" ref="eventAdmin" />
	</bean>
	<bean id="createDirPlugin" class="cz.zcu.kiv.kc.plugin.createDir.CreateDirPlugin">
		<property name="eventAdmin" ref="eventAdmin" />
	</bean>
	<bean id="deleteFilePlugin" class="cz.zcu.kiv.kc.plugin.delete.DeleteFilePlugin">
		<property name="eventAdmin" ref="eventAdmin" />
	</bean>
	<bean id="moveFilePlugin" class="cz.zcu.kiv.kc.plugin.move.MoveFilePlugin">
		<property name="eventAdmin" ref="eventAdmin" />
	</bean>
	<bean id="showImagePlugin" class="cz.zcu.kiv.kc.plugin.showImage.ShowImagePlugin">
		<property name="eventAdmin" ref="eventAdmin" />
	</bean>
	<bean id="showTextPlugin" class="cz.zcu.kiv.kc.plugin.showText.ShowTextPlugin">
		<property name="eventAdmin" ref="eventAdmin" />
	</bean>
	<bean id="showFilePlugin" class="cz.zcu.kiv.kc.plugin.show.ShowFilePlugin">
		<property name="eventAdmin" ref="eventAdmin" />
	</bean>
	<bean id="unzipFilePlugin" class="cz.zcu.kiv.kc.plugin.unzip.UnzipFilePlugin">
		<property name="eventAdmin" ref="eventAdmin" />
	</bean>
	<bean id="zipFilePlugin" class="cz.zcu.kiv.kc.plugin.zip.ZipFilePlugin">
		<property name="eventAdmin" ref="eventAdmin" />
	</bean>

</beans>